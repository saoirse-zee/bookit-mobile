{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "test": "yarn lint && yarn test:unit",
    "test:unit": "node ./node_modules/jest/bin/jest.js --coverage",
    "test:unit-watch": "node ./node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src/",
    "lint:soft": "yarn lint --rule 'no-console: 1'",
    "e2e": "./local-e2e-tests.sh",
    "badge-coverage": "lcov-badge-generator -o ./coverage/badge.svg ./coverage/lcov.info"
  },
  "jest": {
    "preset": "jest-expo",
    "coverageReporters": [
      "json",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "axios": "^0.17.1",
    "expo": "^23.0.0",
    "luxon": "^0.0.19",
    "react": "16.0.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-23.0.0.tar.gz",
    "react-navigation": "^1.0.0-beta.19",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "coverage-percentage": "^0.0.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest-expo": "^23.0.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1"
  },
  "precommit": [
    "lint:soft"
  ],
  "prepush": [
    "lint",
    "test:unit"
  ]
}
