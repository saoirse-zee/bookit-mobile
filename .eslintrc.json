{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    "no-console": 2,
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    "no-use-before-define": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "compat/compat": 0,
    "prefer-promise-reject-errors": 0
  },
  "env": {
    "jest": true
  },
  "globals": { "fetch": false }
}
