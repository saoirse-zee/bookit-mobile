{
  "expo": {
    "name": "Bookit Mobile",
    "description": "A booking app for iOS and Android.",
    "slug": "bookit-mobile",
    "privacy": "unlisted",
    "sdkVersion": "23.0.0",
    "version": "0.0.1",
    "orientation": "portrait",
    "primaryColor": "#ffffff",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "tools.buildit.bookit.mobile",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "tools.buildit.bookit.mobile",
    }
  }
}
